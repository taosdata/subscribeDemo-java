                    "childtable_offset": 0,
                    "interlace_rows": 1,
                    "insert_interval": 1000,
                    "partial_col_num": 0,
                    "disorder_ratio": 0,
                    "disorder_range": 0,
                    "timestamp_step": 1000,
                    "start_timestamp": "now",
                    "sample_format": "csv",
                    "sample_file": "./sample.csv",
                    "use_sample_ts": "no",
                    "tags_file": "",
                    "columns": [
                        { "type": "FLOAT", "name": "current", "count": 1, "max": 12, "min": 8 },
                        { "type": "INT", "name": "voltage", "max": 225, "min": 215 },
                        { "type": "FLOAT", "name": "phase", "max": 1, "min": 0 }
                    ],
                    "tags": [
                        { "name": "group_id", "type": "int" },
                        { "name": "location", "type": "BINARY", "len": 24, "values": ["BJ", "SH", "GZ"] }
                    ]
                }
            ]
        }
    ]
