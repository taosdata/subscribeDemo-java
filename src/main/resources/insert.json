        {
            "dbinfo": {
                "name": "test",
                "drop": "yes",
                "replica": 1,
                "precision": "ms",
                "keep": 3650,
                "minRows": 100,
                "maxRows": 4096,
		"wal_retention_period": 7200,
                "comp": 2,
                "vgroups ":2,
                "duration":10
            },
            "super_tables": [
                {
                    "name": "meters",
                    "child_table_exists": "yes",
